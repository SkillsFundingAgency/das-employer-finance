@using SFA.DAS.EmployerFinance.Web.Filters;

@{ 
    var zendeskApiData = ViewBag.ZendeskApiData as ZendeskApiFilterAttribute.ZendeskApiData ?? new ZendeskApiFilterAttribute.ZendeskApiData();
}

<script nws-csp-add-nonce="true" type="text/javascript">
    zE(function () {
        zE.identify({
            name: '@zendeskApiData.Name',
            email: '@zendeskApiData.Email',
            organization: '@zendeskApiData.Organization'
        });
    });
</script>